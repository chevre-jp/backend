<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title><%= title %></title>
    <!-- BootstrapのCSS読み込み -->
    <link href="/css/bootstrap.css" rel="stylesheet">
    <link href="/css/bootstrap-datepicker.css" rel="stylesheet">
    <link href="/css/chevre-master.css" rel="stylesheet">
    <!-- jQuery読み込み -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
    <!-- BootstrapのJS読み込み -->
    <script src="/js/bootstrap.js"></script>
    <!-- datepicker本体 -->
    <script src="/js/bootstrap-datepicker.js" type="text/javascript"></script>
    <!-- datepicker日本語対応用ファイル -->
    <script src="/js/locales/bootstrap-datepicker.ja.js" type="text/javascript"></script>
  </head>
  <body>
  <!--Header-->
  <div>
    <div>
      <div class="container-fluid">
        <div class="row">
          <div class="col-sm-1">
            <p class="disp_id">
              <%= displayId %>
            </p>
          </div>
          <div class="col-sm-10"  style="padding-top:10px;">
            <div><%= __('Master.SystemName') %></div>
            <div><%= title %></div>
          </div>
          <div class="col-sm-1 hidden-xs">
            <div class="project_img">
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!--Contents-->
  <div class="container">
    <div class="container-fluid">
      <div class="row" style="border:solid 1px silver;">
        <!--左・メニュー-->
        <div class="col-sm-2" name="__left_menu">
          <div class="project_img_small hidden-xs">
          </div>
          <% /*作品マスタ*/ %>
          <div class="menu-group">
            <div><%= __('Master.Title.Film.Name') %></div>
            <div id="Aa-2"><a href="/master/film/add"><%= __('Master.Title.Film.Add.Name') %></a></div>
            <div id="Aa-3"><a href="/master/film/list')"><%= __('Master.Title.Film.List.Name') %></a></div>
          </div>
          <% /*パフォーマンスマスタ*/ %>
          <div class="menu-group">
            <div><%= __('Master.Title.Performance.Name') %></div>
            <div id="Aa-4"><a href=''><%= __('Master.Title.Performance.Add.Name') %></a></div>
          </div>
          <% /*券種マスタ*/ %>
          <div class="menu-group">
            <div><%= __('Master.Title.TicketType.Name') %></div>
            <div id="Aa-5"><a href=''><%= __('Master.Title.TicketType.Add.Name') %></a></div>
            <div id="Aa-6"><a href=''><%= __('Master.Title.TicketType.List.Name') %></a></div>
          </div>
          <% /*券種グループマスタ*/ %>
          <div class="menu-group">
            <div><%= __('Master.Title.TicketTypeGroup.Name') %></div>
            <div id="Aa-7"><a href=''><%= __('Master.Title.TicketTypeGroup.Add.Name') %></a></div>
            <div id="Aa-8"><a href=''><%= __('Master.Title.TicketTypeGroup.List.Name') %></a></div>
          </div>
          <% /*レポート*/ %>
          <div class="menu-group">
            <div><%= __('Master.Title.Report.Name') %></div>
            <div id="Aa-9"><a href=''><%= __('Master.Title.Report.Sales.Name') %></a></div>
            <div id="Aa-10"><a href=''><%= __('Master.Title.Report.ByAccount.Name') %></a></div>
          </div>
        </div>
        <!--右・作品マスタ新規登録-->
        <div class="col-sm-10" style="padding-left:0px;padding-right:0px;" name="__right_contents">
          <div class="container-fluid">
            <div class="row">
              <!--右・タイトル-->
              <div class="col-sm-12 form-header">
                <div class="container-fluid">
                  <div class="row" style="padding-right:10px;">
                    <div class="col-sm-6 form-header-label"><%= title %></div>
                    <div class="col-sm-4 form-header-label" style="text-align:right;">
                      <%- req.staffUser.get('user_id') %>
                    </div>
                    <div class="col-sm-2">
                      <a class="btn btn-logout" href="/master/logout">
                        <% /*ログアウト*/ %>
                        <span><%= __('Button.Logout') %></span>
                      </a>
                    </div>
                  </div>
                </div>
              </div>
              <%- body %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <script>
  $(function(){
      var disp_id = "#<%= displayId %>";
      $(disp_id).html($(disp_id).text());
  });
  </script>
</body>
</html>
