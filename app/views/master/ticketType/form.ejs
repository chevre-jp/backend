
<% /* サイト表示用券種名 */ %>
<div class="form-group ">
    <label for="ticketNameJa" class="col-sm-3 control-label label-title">
        サイト表示用券種名<font color="red">＊</font>
    </label>
    <div class="col-sm-9">
        <input type="text" class="form-control" name="ticketNameJa" id="ticketNameJa" placeholder="" maxlength=64 value=<%= forms.ticketNameJa %>>
        <p class="message">
            全角64文字以内でご入力ください。
        </p>
        <p class="error message text-danger">
        <% if (errors && errors.ticketNameJa) { %>
            <%- errors.ticketNameJa.msg %>
        <% } %>
        </p>
    </div>
</div>

