<%- contentFor('title') %>
座席検索
<%- contentFor('displayId') %>
ST-01
<%- contentFor('styles') %>
<link href="/css/simplePagination.css" rel="stylesheet">
<%- contentFor('scripts') %>
<script src="/js/jquery.simplePagination.js"></script>
<script src="/js/master/commonMaster.js"></script>
<script src="/js/master/commonMasterList.js"></script>
<script src="/js/master/places/seats.js"></script>

<%- contentFor('body') %>

<div class="form-container search p-3 bg-light-gray mb-3">
    <form method="post">
        <%- include('../../common/master/_hidden') %>
        <div class="menu-group">
        </div>

        <div class="form-group">
            <label for="containedInPlace[containedInPlace][containedInPlace][branchCode][$eq]" class="">
                劇場
            </label>
            <select class="form-control" name="containedInPlace[containedInPlace][containedInPlace][branchCode][$eq]">
                <option value="">選択してください</option>
                <% for (const movieTheater of movieTheaters) { %>
                <option value="<%= movieTheater.branchCode %>">
                    <%= movieTheater.name.ja %>
                </option>
                <% } %>
            </select>
        </div>

        <div class="form-group">
            <label for="branchCode[$eq]" class="">
                座席コード
            </label>
            <input class="form-control" name="branchCode[$eq]">
        </div>

        <div>
            <!-- <a class="btn btn-primary" role="button" href="/places/movieTheater/new">
                <i class=" fa fa-plus"></i> 登録</a> -->

            <button type="button" class="reset-condition btn btn-secondary float-right">検索内容リセット</button>
            <button type="button" class="btn search btn-ok btn-success float-right">検索</button>
        </div>
    </form>
</div>

<div class="form-container search">
    <p id="resultStats" style="display:none;">
    </p>
    <!-- <p class="mb-3" id="searchedCount" style="display:none;">
        検索結果 $searched_count$件
    </p> -->
    <div id="list" style="display:none;">

        <div class="card">
            <div class="card-body">
                <div class="table-responsive">
                    <table class="table">
                        <thead class="text-primary">
                            <tr>
                                <th>コード</th>
                                <th>劇場</th>
                                <th>スクリーン</th>
                                <th>セクション</th>
                                <th>座席タイプ</th>
                                <th></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr id="templateRow" identifier="">
                                <td
                                    name="typeOf__containedInPlace.containedInPlace.containedInPlace.branchCode__containedInPlace.containedInPlace.branchCode__containedInPlace.branchCode__branchCode__branchCode">
                                    <span class="badge badge-secondary">$typeOf$</span><br>
                                    <a target="_blank"
                                        href="/places/seat/$containedInPlace.containedInPlace.containedInPlace.branchCode$:$containedInPlace.containedInPlace.branchCode$:$containedInPlace.branchCode$:$branchCode$/update">$branchCode$</a>
                                </td>
                                <td
                                    name="containedInPlace.containedInPlace.containedInPlace.typeOf__containedInPlace.containedInPlace.containedInPlace.branchCode__containedInPlace.containedInPlace.containedInPlace.name.ja">
                                    <span
                                        class="badge badge-secondary">$containedInPlace.containedInPlace.containedInPlace.typeOf$</span><br>
                                    $containedInPlace.containedInPlace.containedInPlace.branchCode$<br>
                                    $containedInPlace.containedInPlace.containedInPlace.name.ja$<br>
                                </td>
                                <td
                                    name="containedInPlace.containedInPlace.typeOf__containedInPlace.containedInPlace.branchCode__containedInPlace.containedInPlace.name.ja">
                                    <span
                                        class="badge badge-secondary">$containedInPlace.containedInPlace.typeOf$</span><br>
                                    $containedInPlace.containedInPlace.branchCode$<br>
                                    $containedInPlace.containedInPlace.name.ja$<br>
                                </td>
                                <td
                                    name="containedInPlace.typeOf__containedInPlace.branchCode__containedInPlace.name.ja">
                                    <span class="badge badge-secondary">$containedInPlace.typeOf$</span><br>
                                    $containedInPlace.branchCode$<br>
                                    $containedInPlace.name.ja$<br>
                                </td>
                                <td name="seatingType">
                                    $seatingType$<br>
                                </td>
                                <td name="branchCode__id" class="">
                                    <!-- <div class="text-center">
                                        <a href="javascript:void(0)"
                                            class="btn btn-sm btn-outline-primary showAdditionalProperty"
                                            data-branchCode="$branchCode$">
                                            <span>追加特性</span>
                                        </a>
                                    </div> -->
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>

    </div>
    <div id="pager" class="div_pager_parent"></div>
</div>

<div class="modal fade" id="modal-additionalProperty" tabindex="-1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span>×</span></button>
                <h4 class="modal-title">追加特性</h4>
            </div>
            <div class="modal-body">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">閉じる</button>
            </div>
        </div>
    </div>
</div>

<div class="modal fade" id="modal-containsPlace" tabindex="-1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span>×</span></button>
                <h4 class="modal-title">スクリーン情報</h4>
            </div>
            <div class="modal-body">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">閉じる</button>
            </div>
        </div>
    </div>
</div>

<%- include('../../common/master/_loading') %>