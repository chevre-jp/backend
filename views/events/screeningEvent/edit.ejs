<div class="modal fade" id="editModal" tabindex="-1">
    <div class="modal-dialog modal-dialog-scrollable modal-dialog-centered modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">スケジュール編集</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="panel-body">
                    <form class="p-3 bg-light-gray">
                        <ul class="list-group mb-3">
                            <li class="list-group-item film">作品 : <span></span></li>
                            <li class="list-group-item theater">劇場 : <span></span></li>
                            <li class="list-group-item day">上映日 : <span></span></li>
                        </ul>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0"
                                    for="doorTime">開場時刻</label>
                                <div class="col-md-8 form-inline">
                                    <%- include('../../common/part/_timeSelector', {
                                            hourName: 'doorTimeHour',
                                            minuteName: 'doorTimeMinutes'
                                        }) %>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0"
                                    for="startTime">開始時刻</label>
                                <div class="col-md-8 form-inline">
                                    <%- include('../../common/part/_timeSelector', {
                                            hourName: 'startTimeHour',
                                            minuteName: 'startTimeMinutes'
                                        }) %>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0"
                                    for="endTime">終了時刻</label>
                                <div class="col-md-8 form-inline">
                                    <%- include('../../common/part/_timeSelector', {
                                            hourName: 'endTimeHour',
                                            minuteName: 'endTimeMinutes'
                                        }) %>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0"
                                    for="screen">スクリーン</label>
                                <div class="col-md-8 ">
                                    <select class="form-control" name="screen"></select>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0"
                                    for="ticketTypeGroup">券種グループ</label>
                                <div class="col-md-8 ">
                                    <select class="form-control" name="ticketTypeGroup">
                                        <option value="">選択してください</option>
                                        <% for (ticketType of ticketGroups) { %>
                                        <option value="<%= ticketType.id %>">
                                            <%= ticketType.name.ja %>
                                        </option>
                                        <% } %>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0"
                                    for="saleStartDate">販売開始日</label>
                                <div class="col-md-8 form-inline">
                                    <input class="form-control datepicker" name="saleStartDate" placeholder="YYYY/MM/DD"
                                        maxlength=10>
                                    日
                                    <%- include('../../common/part/_timeSelector', {
                                        hourName: 'saleStartDateHour',
                                        minuteName: 'saleStartDateMinutes'
                                    }) %>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0"
                                    for="onlineDisplayStartDate">オンライン表示開始日</label>
                                <div class="col-md-8 form-inline">
                                    <input class="form-control datepicker" name="onlineDisplayStartDate"
                                        placeholder="YYYY/MM/DD" maxlength=10>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0"
                                    for="maxSeatNumber">上限枚数</label>
                                <div class="col-md-8">
                                    <input class="form-control" type="number" name="maxSeatNumber" maxlength=10>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label for="name" class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0">
                                </label>
                                <div class="col-md-8 form-check">
                                    <label class="form-check-label" for="mvtkExcludeFlg">
                                        <input class="form-check-input" type="checkbox" name="mvtkExcludeFlg" value="1"
                                            id="mvtkExcludeFlg">
                                        ムビチケ対象外
                                        <span class="form-check-sign">
                                            <span class="check"></span>
                                        </span>
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <div class="row">
                                <label class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0">
                                </label>
                                <div class="col-md-8 form-check">
                                    <input type="hidden" value="1" name="reservedSeatsAvailable">
                                    <label class="form-check-label" for="reservedSeatsAvailable">
                                        <input disabled class="form-check-input" type="checkbox"
                                            name="reservedSeatsAvailableDisabled" value="1"
                                            id="reservedSeatsAvailableDisabled">
                                        座席指定あり
                                        <span class="form-check-sign">
                                            <span class="check"></span>
                                        </span>
                                    </label>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
            <input type="hidden" name="screeningEventId" value="">
            <input type="hidden" name="performance" value="">
            <input type="hidden" name="film" value="">
            <input type="hidden" name="theater" value="">
            <input type="hidden" name="day" value="">
            <div class="modal-footer">

                <button type="button" class="btn btn-primary update-button">更新</button>
                <button type="button" class="btn btn-danger delete-button">削除</button>
                <button type="button" class="btn bg-dark-gray text-white" data-dismiss="modal">閉じる</button>

            </div>
        </div>
    </div>
</div>