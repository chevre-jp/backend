<div class="form-group">
    <label for="typeOf" class="">
        価格仕様タイプ
    </label>
    <select class="form-control" name="typeOf">
        <option value="" disabled selected>選択してください</option>
        <% Object.values(PriceSpecificationType).forEach((type) => { %>
        <option value="<%= type %>" <% if (type === forms.typeOf) { %> selected<% } %>>
            <%= type %>
        </option>
        <% }) %>
    </select>
</div>

<div class="form-group">
    <label for="price" class="">
        金額<span class="text-danger">＊</span>
    </label>
    <input type="text" class="form-control" name="price" id="price" placeholder="" maxlength=10 value=<%=forms.price %>>
    <p class="message">
        半角10文字以内でご入力ください。
    </p>
    <p class="error message text-danger">
        <% if (errors && errors.price) { %>
        <%- errors.price.msg %>
        <% } %>
    </p>
</div>

<div class="form-group">
    <label for="appliesToMovieTicketType" class="">
        適用ムビチケ券種区分
    </label>
    <select class="form-control" name="appliesToMovieTicketType">
        <option value="" disabled selected>選択してください</option>
        <% Object.values(MovieTicketType).forEach((type) => { %>
        <option value="<%= type.code %>" <% if (type.code === forms.appliesToMovieTicketType) { %> selected<% } %>>
            <%= type.code %> <%= type.name %>
        </option>
        <% }) %>
    </select>
</div>

