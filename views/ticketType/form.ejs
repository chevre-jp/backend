<div class="form-group ">
    <label for="name[ja]" class="col-sm-3 control-label label-title">
        サイト表示用券種名<font color="red">＊</font>
    </label>
    <div class="col-sm-9">
        <input type="text" class="form-control" name="name[ja]" id="name[ja]" placeholder="" maxlength=64 value=<%=forms.name.ja
            %>>
        <p class="message">
            全角64文字以内でご入力ください。
        </p>
        <p class="error message text-danger">
            <% if (errors && errors['name.ja']) { %>
            <%- errors['name.ja'].msg %>
            <% } %>
        </p>
    </div>
</div>
<div class="form-group ">
    <label for="name[en]" class="col-sm-3 control-label label-title">
        サイト表示用券種名英<font color="red">＊</font>
    </label>
    <div class="col-sm-9">
        <input type="text" class="form-control" name="name[en]" id="name[en]" placeholder="" maxlength=64 value=<%=forms.name.en
            %>>
        <p class="message">
            半角64文字以内でご入力ください。
        </p>
        <p class="error message text-danger">
            <% if (errors && errors['name.en']) { %>
            <%- errors['name.en'].msg %>
            <% } %>
        </p>
    </div>
</div>
<div class="form-group ">
    <label for="price" class="col-sm-3 control-label label-title">
        金額<font color="red">＊</font>
    </label>
    <div class="col-sm-9">
        <input type="text" class="form-control" name="price" id="price" placeholder="" maxlength=10 value=<%=forms.price
            %>>
        <p class="message">
            半角10文字以内でご入力ください。
        </p>
        <p class="error message text-danger">
            <% if (errors && errors.price) { %>
            <%- errors.price.msg %>
            <% } %>
        </p>
    </div>
</div>
<div class="form-group">
    <label for="description[ja]" class="col-sm-3 control-label label-title">
        補足説明
    </label>
    <div class="col-sm-9">
        <input type="text" class="form-control" name="description[ja]" id="description[ja]" placeholder="" maxlength=64
            value=<%=forms.description.ja %>>
        <p class="message">
            全角64文字以内でご入力ください。
        </p>
        <p class="error message text-danger">
            <% if (errors && errors['description.ja']) { %>
            <%- errors['description.ja'].msg %>
            <% } %>
        </p>
    </div>
</div>
<div class="form-group ">
    <label for="description[en]" class="col-sm-3 control-label label-title">
        補足説明英
    </label>
    <div class="col-sm-9">
        <input type="text" class="form-control" name="description[en]" id="description[en]" placeholder="" maxlength=64
            value=<%=forms.description.en %>>
        <p class="message">
            半角64文字以内でご入力ください。
        </p>
        <p class="error message text-danger">
            <% if (errors && errors['description.en']) { %>
            <%- errors['description.en'].msg %>
            <% } %>
        </p>
    </div>
</div>
<div class="form-group ">
    <label for="indicatorColor" class="col-sm-3 control-label label-title">
        入場時表示カラー
    </label>
    <div class="col-sm-9">
        <input type="text" class="form-control cp-preventtouchkeyboardonshow" name="indicatorColor" id="indicatorColor"
            style="background: rgb(0, 0, 255); color: rgb(255, 255, 255);" title="">
        <input type="hidden" name="hiddenColor" id="hiddenColor">
    </div>
</div>
<div class="form-group ">
    <label for="indicatorColor" class="col-sm-3 control-label label-title">
        在庫状況
    </label>
    <div class="col-sm-9">
        <select class="form-control" name="availability">
            <option value="<%= ItemAvailability.InStock %>" <% if (forms.availability===ItemAvailability.InStock) { %>
                selected
                <% } %>>指定なし</option>
            <option value="<%= ItemAvailability.InStoreOnly %>" <% if (forms.availability===ItemAvailability.InStoreOnly)
                { %> selected
                <% } %>>
                店頭販売のみ
            </option>
            <option value="<%= ItemAvailability.OnlineOnly %>" <% if (forms.availability===ItemAvailability.OnlineOnly)
                { %> selected
                <% } %>>
                オンライン販売のみ
            </option>
        </select>
    </div>
</div>
<div class="form-group ">
    <label for="eligibleQuantity[value]" class="col-sm-3 control-label label-title">
        価格単位<font color="red">＊</font>
    </label>
    <div class="col-sm-9">
        <% const choices = [1,2,3,4] %>
        <select class="form-control" name="eligibleQuantity[value]" id="eligibleQuantity[value]">
            <% choices.forEach((choice) => { %>
            <option value="<%= choice %>" <% if (forms.eligibleQuantity.value === choice) { %> selected
                <% } %>>
                <%= choice %>
            </option>
            <% }) %>
        </select>
        <p class="error message text-danger"></p>
    </div>
</div>