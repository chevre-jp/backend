<div class="form-group row">
    <label for="name[ja]" class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0">
        券種の種別
    </label>
    <div class="col-md-8">
        <select class="form-control" name="category">
            <option value="" disabled selected>選択してください</option>
            <% for (let ticketTypeCategory of ticketTypeCategories) { %>
            <option value="<%= ticketTypeCategory.id %>" <% if (forms.category === ticketTypeCategory.id ) { %> selected
                <% } %>>
                <%= ticketTypeCategory.name %>
            </option>
            <% } %>
        </select>
    </div>
</div>

<div class="form-group row">
    <label for="name[ja]" class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0">
        サイト表示用券種名<span class="text-danger">＊</span>
    </label>
    <div class="col-md-8">
        <input type="text" class="form-control" name="name[ja]" id="name[ja]" placeholder="" maxlength=64 value=<%=forms.name.ja
            %>>
        <p class="message">
            全角64文字以内でご入力ください。
        </p>
        <p class="error message text-danger">
            <% if (errors && errors['name.ja']) { %>
            <%- errors['name.ja'].msg %>
            <% } %>
        </p>
    </div>
</div>

