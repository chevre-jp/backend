<div class="form-group row">
    <label for="seatReservationUnit" class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0">
        適用単位<span class="text-danger">＊</span>
    </label>
    <div class="col-md-8">
        <select class="form-control" name="seatReservationUnit">
            <option value="" disabled selected>選択してください</option>
            <% for (let i = 1; i <= 10; i++) { %>
            <option value="<%= i %>" <% if (forms.seatReservationUnit==i) { %> selected <% } %>>
                <%= i %>
            </option>
            <% } %>
        </select>
        <p class="error message text-danger">
            <% if (errors && errors.seatReservationUnit) { %>
            <%- errors.seatReservationUnit.msg %>
            <% } %>
        </p>
    </div>
</div>

<div class="form-group row">
    <label for="priceSpecification[referenceQuantity][minValue]"
        class="col-md-4 pt-0 pt-md-3 text-md-right mb-2 mb-md-0">
        適用最小数
    </label>
    <div class="col-md-8">
        <select class="form-control" name="priceSpecification[referenceQuantity][minValue]">
            <option value="" selected>指定しない</option>
            <% for (let i = 1; i <= 50; i++) { %>
            <option value="<%= i %>" <% if (Number(forms.priceSpecification.referenceQuantity.minValue) === i) { %>
                selected <% } %>>
                <%= i %>
            </option>
            <% } %>
        </select>
        <p class="error message text-danger">
        </p>
    </div>
</div>

