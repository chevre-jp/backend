<%- contentFor('title') %>
科目分類検索
<%- contentFor('displayId') %>
AT-05
<%- contentFor('styles') %>
<link href="/css/simplePagination.css" rel="stylesheet">
<%- contentFor('scripts') %>
<script src="/js/jquery.simplePagination.js"></script>
<script src="/js/master/commonMaster.js"></script>
<script src="/js/master/commonMasterList.js"></script>
<script src="/js/master/accountTitles/accountTitleCategory.js"></script>

<%- contentFor('body') %>

<div class="col-sm-12 form-container search searchAccountTitleCategories">
    <form method="post" class="form-horizontal">
        <%- include('../../common/master/_hidden') %>

        <div class="form-group ">
            <label for="codeValue" class="col-sm-3 control-label label-title">
                科目分類コード
            </label>
            <div class="col-sm-9">
                <input type="text" class="form-control" name="codeValue" id="codeValue" placeholder="" maxlength=64
                    value="">
            </div>
        </div>
        <div class="form-group">
            <div class="panel-body">
                <a class="btn btn-ok search btn-primary"><span>検索する</span></a>
            </div>
        </div>
    </form>
</div>

<div class="col-sm-12 form-container search">
    <div id="list" class="table-responsive" style="display:none;">
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>科目分類コード</th>
                    <th>科目分類名称</th>
                </tr>
            </thead>
            <tbody>
                <tr id="templateRow">
                    <td name="codeValue"><a class="editAccountTitleCategory" href="javascript:void(0)">$codeValue$</a>
                    </td>
                    <td name="name">$name$</td>
                </tr>
            </tbody>
        </table>
        <div id="pager" class="div_pager_parent"></div>
    </div>
</div>

<div class="modal fade" id="modal-additionalProperty" tabindex="-1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span>×</span></button>
                <h4 class="modal-title">細目の追加特性</h4>
            </div>
            <div class="modal-body">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">閉じる</button>
            </div>
        </div>
    </div>
</div>

<%- include('../../common/master/_loading') %>