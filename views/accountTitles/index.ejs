<%- contentFor('title') %>
細目マスタ一覧
<%- contentFor('displayId') %>
AT-01
<%- contentFor('styles') %>
<link href="/css/simplePagination.css" rel="stylesheet">
<%- contentFor('scripts') %>
<script src="/js/jquery.simplePagination.js"></script>
<script src="/js/master/commonMaster.js"></script>
<script src="/js/master/commonMasterList.js"></script>
<script src="/js/master/accountTitles.js"></script>

<%- contentFor('body') %>

<div class="col-sm-12 form-container search">
    <form method="post" class="form-horizontal">
        <%- include('../common/master/_hidden') %>
        <div class="menu-group">
            <% if (forms.message !== undefined) { %>
            <p class="error message text-danger panel-body bg-danger">
                <%= forms.message %>
            </p>
            <% } %>
        </div>
        <div class="form-group">
            <label for="inCodeSet[inCodeSet][codeValue]" class="col-sm-3 control-label label-title">
                科目分類
            </label>
            <div class="col-sm-9">
                <select class="form-control" name="inCodeSet[inCodeSet][codeValue]">
                    <option value="">選択してください</option>
                    <% for (const accountTitleCategory of accountTitleCategories) { %>
                    <option value="<%= accountTitleCategory.codeValue %>">
                        <%= accountTitleCategory.codeValue %>
                        <%= accountTitleCategory.name %>
                    </option>
                    <% } %>
                </select>
            </div>
        </div>
        <div class="form-group">
            <label for="inCodeSet[codeValue]" class="col-sm-3 control-label label-title">
                科目
            </label>
            <div class="col-sm-9">
                <select class="form-control" name="inCodeSet[codeValue]">
                    <option value="">選択してください</option>
                    <% for (const accountTitleSet of accountTitleSets) { %>
                    <option value="<%= accountTitleSet.codeValue %>">
                        <%= accountTitleSet.codeValue %>
                        <%= accountTitleSet.name %>
                    </option>
                    <% } %>
                </select>
            </div>
        </div>
        <div class="form-group ">
            <label for="codeValue" class="col-sm-3 control-label label-title">
                細目コード
            </label>
            <div class="col-sm-9">
                <input type="text" class="form-control" name="codeValue" id="codeValue" placeholder="" maxlength=64
                    value="">
            </div>
        </div>
        <div class="form-group">
            <div class="panel-body">
                <a class="btn btn-ok search btn-primary"><span>検索する</span></a>
                <a class="reset-condition btn btn-danger"><span>検索内容リセット</span></a>
            </div>
        </div>
    </form>
</div>

<div class="col-sm-12 form-container search">
    <div>
        <p id="searchedCount" style="display:none;">
            検索結果 $searched_count$件
        </p>
    </div>
    <div id="list" class="table-responsive" style="display:none;">
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>科目分類コード</th>
                    <th>科目分類名称</th>
                    <th>科目コード</th>
                    <th>科目名称</th>
                    <th>細目コード</th>
                    <th>細目名称</th>
                </tr>
            </thead>
            <tbody>
                <tr id="templateRow" codeValue="$codeValue$">
                    <td name="inCodeSet.inCodeSet.codeValue"><a class="editAccountTitleCategory" href="#">$inCodeSet.inCodeSet.codeValue$</a></td>
                    <td name="inCodeSet.inCodeSet.name">$inCodeSet.inCodeSet.name$</td>
                    <td name="inCodeSet.codeValue"><a class="editAccountTitleSet" href="#">$inCodeSet.codeValue$</a></td>
                    <td name="inCodeSet.name">$inCodeSet.name$</td>
                    <td name="codeValue"><a class="editAccountTitle" href="#">$codeValue$</a></td>
                    <td name="name">$name$</td>
                </tr>
            </tbody>
        </table>
    </div>
    <div id="pager" class="div_pager_parent"></div>
</div>

<%- include('../common/master/_loading') %>