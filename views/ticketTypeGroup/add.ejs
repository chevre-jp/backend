<%- contentFor('title') %>
券種グループマスタ新規登録
<%- contentFor('displayId') %>
Aa-7
<%- contentFor('styles') %>
<link rel="stylesheet" href="/css/ticketTypeGroupAddEdit.css">
<%- contentFor('scripts') %>
<script src="/js/jquery-ui.js"></script>
<script src="/js/master/ticketTypeGroup/ticketTypeGroupAddEdit.js"></script>

<%- contentFor('body') %>
<div class="form-container">
    <form method="post" class="p-3 bg-light-gray" id="ticketTypeGroupsForm">
        <div class="menu-group">
            <%- include('../common/errorMessage') %>
            <%- include('../common/successMessage') %>
        </div>

        <div class="form-group row">
            <label for="id" class="">
                券種グループコード<span class="text-danger">＊</span>
            </label>
            <input type="text" class="form-control" name="id" id="id" placeholder="" maxlength=64
                value="<%= forms.id %>">
            <p class="message">
                半角64文字以内でご入力ください。
            </p>
            <p class="error message text-danger">
                <% if (errors && errors.id) { %>
                <%- errors.id.msg %>
                <% } %>
            </p>
        </div>

        <%- include('form', { forms: forms }) %>

        <div class="btn-area panel-body">
            <button type="button" class="btn btn-ok btn-primary">登録する</button>
        </div>
    </form>
</div>