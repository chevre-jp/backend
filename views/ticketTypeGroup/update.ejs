<%- contentFor('title') %>
券種グループマスタ更新
<%- contentFor('displayId') %>
Aa-7
<%- contentFor('styles') %>
<link rel="stylesheet" href="/css/ticketTypeGroupAddEdit.css">
<%- contentFor('scripts') %>
<script src="/js/jquery-ui.js"></script>
<script src="/js/master/ticketTypeGroup/ticketTypeGroupAddEdit.js"></script>

<%- contentFor('body') %>
<div class="form-container">
    <form method="post" class="p-3 bg-light-gray" id="ticketTypeGroupsForm">
        <div class="menu-group">
            <%- include('../common/errorMessage') %>
            <%- include('../common/successMessage') %>
        </div>

        <input type="hidden" name="id" value="<%= forms.id %>">
        <input type="hidden" name="identifier" value="<%= forms.identifier %>">

        <%- include('form', { forms: forms }) %>

        <div class="btn-area panel-body">
            <button type="button" class="btn btn-ok btn-primary">更新する</button>
            <button type="button" class="btn btn-delete btn-danger">削除する</button>
        </div>
    </form>
</div>